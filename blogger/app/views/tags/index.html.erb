<h1>
    <ul id="tags">
        <% @tags.each do |tag| %>
            <li>
            <%= link_to tag.name, tag_path(tag) %>
            <%= link_to "Destroy tag #{tag.name}.", tag_path(tag), method: :delete, data: {confirm: "Really delete this tag?"} %>
            </li>
        <% end %>
    </ul>
</h1>
